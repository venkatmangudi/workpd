<%= simple_form_for @district, :validate => true, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.association :division,label_method: :division_name, value_method: :id, include_blank: false %>
  <%= f.input :district_name %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                districts_path, :class => 'btn' %>
  </div>
<% end %>
