<%= simple_form_for @employee_sanction_working, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :employee_cadre_id %>
  <%= f.input :employee_id %>
  <%= f.input :emp_full_name %>
  <%= f.input :gender_name %>
  <%= f.input :join_date %>
  <%= f.input :employee_class %>
  <%= f.input :qualification_name_id %>
  <%= f.input :qualification_name %>
  <%= f.input :specialisation_id %>
  <%= f.input :specialisation_name %>
  <%= f.input :designation_id %>
  <%= f.input :Designation_English %>
  <%= f.input :Designation_hindi %>
  <%= f.input :employment_type_id %>
  <%= f.input :employment_type %>
  <%= f.input :status_id %>
  <%= f.input :status_name %>
  <%= f.input :district_id %>
  <%= f.input :district_name %>
  <%= f.input :hospital_id %>
  <%= f.input :hospital_name %>
  <%= f.input :sanctioned_posts %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                employee_sanction_workings_path, :class => 'btn' %>
  </div>
<% end %>
